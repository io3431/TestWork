@model CardModelDataView

<div class="card">
    <h5 class="card-header">Информация о карте</h5>
    <div class="card-body d-flex justify-content-center">
        <div class="row g-3">
            <div class="col-sm-6">
                <label for="cardcode" class="form-label">Номер карты: @Model.CardCode</label>
            </div>

            <div class="col-12">
                <label for="cardtype" class="form-label">Тип карты: @Model.CardType</label>
            </div>

            <div class="col-12">
                <label for="ownerguid" class="form-label">OwnerGUID: @Model.OwnerGUID</label>
            </div>

            <div class="col-12">
                <label for="cardper" class="form-label">Cardper: @Model.CardPer</label>
            </div>

            <div class="col-12">
                <label for="turnover" class="form-label">TurnOver: @Model.TurnOver</label>
            </div>
        </div>
    </div>
</div>
